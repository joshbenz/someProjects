<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.csun.a380group5.fitnessapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WorkoutGenerator.WorkoutGeneratorActivity" />
        <activity android:name=".WorkoutGenerator.ExerciseListActivity" />
        <activity android:name=".WorkoutGenerator.ExerciseDataActivity" />
        <activity android:name=".WorkoutGenerator.WebViewExercise" />
        <activity android:name=".WorkoutGenerator.LoadedExercisesActivity"></activity>
    </application>

</manifest>